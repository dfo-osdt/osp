{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "laracasts/cypress": "^3.0",
        "laravel/framework": "^9.42.2",
        "laravel/sanctum": "^3.0.1",
        "laravel/telescope": "^4.9.6",
        "laravel/tinker": "^2.7.3",
        "orhanerday/open-ai": "^3.2",
        "owen-it/laravel-auditing": "^13.0.5",
        "predis/predis": "^2.0.3",
        "spatie/laravel-medialibrary": "^10.7.3",
        "spatie/laravel-permission": "^5.7",
        "spatie/laravel-query-builder": "^5.1.1",
        "spatie/laravel-ray": "^1.31",
        "spatie/simple-excel": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12.3",
        "fakerphp/faker": "^1.20",
        "laravel/breeze": "^1.15.3",
        "laravel/pint": "^1.2.1",
        "laravel/sail": "^1.16.3",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^6.3.1",
        "orangehill/iseed": "^3.0.2",
        "pestphp/pest": "^1.22.2",
        "pestphp/pest-plugin-laravel": "^1.3",
        "phpunit/phpunit": "^9.5.26",
        "spatie/laravel-ignition": "^1.6.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
